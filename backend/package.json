    "styled-components": "^5.3.6",
    "web-vitals": "^3.1.1",
    "yup": "^1.0.0"
  },
  "scripts": {
    "start": "HTTPS=true SSL_CRT_FILE=/etc/apache2/ssl/server.crt SSL_KEY_FILE=/etc/apache2/ssl/private.key PORT=3000 react-scripts start",
    "build": "react-scripts build --max_old_space_size=8000 build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.{js,jsx,json,md,html,css,less,scss}\""
  },
  "babelOptions": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "react-app"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false,
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "globals": {
      "$": true
    },
    "settings": {
      "react": {
        "version": "^17.0.1"
      }
    },
    "rules": {
      "semi": 0,
      "react/no-unescaped-entities": 0,
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off",
      "no-unused-vars": "off",
      "no-extra-boolean-cast": 0,
      "react/no-unknown-property": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "moment": "2.29.1",
    "autoprefixer": "10.4.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "babel-plugin-prismjs": "^2.1.0",
    "css-loader": "^7.1.2",
    "postcss-loader": "^8.1.1",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^16.0.2"
  },
  "main": "index.js",
  "license": "MIT"
}
